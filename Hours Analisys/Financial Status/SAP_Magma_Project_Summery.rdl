    <DataSet Name="ORDERS">
      <Query>
        <DataSourceName>MagmaSAP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@intTaskCode">
            <Value>=Parameters!intTaskCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     dbo.ORDR.CardName, dbo.ORDR.Project, dbo.OPRJ.PrjName, dbo.ORDR.DocStatus, dbo.ORDR.DocNum,  dbo.ORDR.GrosProfit, (dbo.ORDR.DocTotal - dbo.ORDR.VatSum) AS DocTotal, dbo.ORDR.ImportEnt, dbo.ORDR.NumAtCard, dbo.ORDR.DocDate, dbo.ORDR.PaidToDate, dbo.ORDR.VatSum
FROM         dbo.ORDR INNER JOIN
                      dbo.OPRJ ON dbo.ORDR.Project = dbo.OPRJ.PrjCode
WHERE     (dbo.ORDR.Project=@intTaskCode)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
