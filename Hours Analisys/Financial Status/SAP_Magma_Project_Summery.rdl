    <DataSet Name="TASK_INFO">
      <Query>
        <DataSourceName>MagmaSAP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@intTaskCode">
            <Value>=Parameters!intTaskCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT T1.PrjCode AS TaskCode, T1.PrjName AS Description, T2.CardName AS CompanyName, 0 AS PlannedHours
FROM   ORDR AS T0 INNER JOIN
          OPRJ AS T1 ON T0.Project = T1.PrjCode INNER JOIN
          OCRD AS T2 ON T0.CardCode = T2.CardCode
WHERE  (T1.PrjCode = @intTaskCode)</CommandText>
        <Timeout>30</Timeout>
      </Query>
      <Fields>
        <Field Name="TaskCode">
          <DataField>TaskCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
