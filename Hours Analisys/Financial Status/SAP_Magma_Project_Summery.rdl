    <DataSet Name="SUPPLIER_INVOICES">
      <Query>
        <DataSourceName>MagmaSAP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@intTaskCode">
            <Value>=Parameters!intTaskCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT T0.DocNum,
       T0.ImportEnt,
       T0.NumAtCard,
       T1.CardName,
       T0.Project,
       T1.CardCode,
       T0.DocDate,
       T0.DocDueDate,
       T0.PaidToDate,
       T0.DocTotal,
       (T0.[DocTotal] - T0.[VatSum]) AS 'NoVAT',
	   T0.DocStatus
FROM [dbo].[OCRD] T1
    INNER JOIN [dbo].[OPCH] T0
        ON T1.CardCode = T0.CardCode
WHERE T0.Project = @intTaskCode
UNION ALL
SELECT T0.DocNum,
       T0.ImportEnt,
       T0.NumAtCard,
       T1.CardName,
       T0.Project,
       T1.CardCode,
       T0.DocDate,
       T0.DocDueDate,
       T0.PaidToDate,
       T0.DocTotal,
       (T0.[DocTotal] - T0.[VatSum]) AS 'NoVAT',
	   T0.DocStatus
FROM [dbo].[OCRD] T1
    INNER JOIN [dbo].[ORPC] T0
        ON T1.CardCode = T0.CardCode
WHERE T0.Project = @intTaskCode</CommandText>
