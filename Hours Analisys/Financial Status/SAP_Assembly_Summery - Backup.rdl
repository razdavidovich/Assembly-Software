        <Field Name="Total_Equite">
          <DataField>Total Equite</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Issued">
          <DataField>Total Issued</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AssemblySAP</DataSourceName>
        <CommandText>SELECT     dbo.Base_Order_Info_Ve.CardCode, dbo.Base_Order_Info_Ve.CardName, dbo.Base_Order_Info_Ve.Project, dbo.Base_Order_Info_Ve.PrjName, 
                      dbo.Base_Order_Info_Ve.DocNum, dbo.Base_Order_Info_Ve.GrosProfSy, dbo.Base_Order_Info_Ve.DocTotalSy, 
                      ISNULL(dbo.Total_Invoice_To_Order_Ve.DocTotalSy_Sum, 0) - ISNULL(dbo.Total_Invoice_To_Order_Ve.VatSumSy_Sum, 0) AS [Total Invoices$], 
                      ISNULL(dbo.Total_Equite_To_Order_Ve.DocTotalSy_Sum, 0) - ISNULL(dbo.Total_Equite_To_Order_Ve.VatSumSy_Sum, 0) AS [Total Equite$], 
                      (ISNULL(dbo.Total_Invoice_To_Order_Ve.DocTotalSy_Sum, 0) - ISNULL(dbo.Total_Invoice_To_Order_Ve.VatSumSy_Sum, 0)) 
                      - (ISNULL(dbo.Total_Equite_To_Order_Ve.DocTotalSy_Sum, 0) - ISNULL(dbo.Total_Equite_To_Order_Ve.VatSumSy_Sum, 0)) AS [Total Issued$], 
                      dbo.Base_Order_Info_Ve.DocTotalSy - ((ISNULL(dbo.Total_Invoice_To_Order_Ve.DocTotalSy_Sum, 0) 
                      - ISNULL(dbo.Total_Invoice_To_Order_Ve.VatSumSy_Sum, 0)) - (ISNULL(dbo.Total_Equite_To_Order_Ve.DocTotalSy_Sum, 0) 
                      - ISNULL(dbo.Total_Equite_To_Order_Ve.VatSumSy_Sum, 0))) AS Balance$, ISNULL(dbo.Total_Invoice_To_Order_Ve.DocTotal_Sum, 0) 
                      - ISNULL(dbo.Total_Invoice_To_Order_Ve.VatSum_Sum, 0) AS [Total Invoices], ISNULL(dbo.Total_Equite_To_Order_Ve.DocTotal_Sum, 0) 
                      - ISNULL(dbo.Total_Equite_To_Order_Ve.VatSum_Sum, 0) AS [Total Equite], (ISNULL(dbo.Total_Invoice_To_Order_Ve.DocTotal_Sum, 0) 
                      - ISNULL(dbo.Total_Invoice_To_Order_Ve.VatSum_Sum, 0)) - (ISNULL(dbo.Total_Equite_To_Order_Ve.DocTotal_Sum, 0) 
                      - ISNULL(dbo.Total_Equite_To_Order_Ve.VatSum_Sum, 0)) AS [Total Issued], 
                      dbo.Base_Order_Info_Ve.DocTotal - ((ISNULL(dbo.Total_Invoice_To_Order_Ve.DocTotal_Sum, 0) 
                      - ISNULL(dbo.Total_Invoice_To_Order_Ve.VatSum_Sum, 0)) - (ISNULL(dbo.Total_Equite_To_Order_Ve.DocTotal_Sum, 0) 
                      - ISNULL(dbo.Total_Equite_To_Order_Ve.VatSum_Sum, 0))) AS Balance
FROM         dbo.Base_Order_Info_Ve LEFT OUTER JOIN
                      dbo.Total_Invoice_To_Order_Ve ON dbo.Base_Order_Info_Ve.ImportEnt = dbo.Total_Invoice_To_Order_Ve.ImportEnt LEFT OUTER JOIN
                      dbo.Total_Equite_To_Order_Ve ON dbo.Base_Order_Info_Ve.ImportEnt = dbo.Total_Equite_To_Order_Ve.ImportEnt
FOR BROWSE</CommandText>
        <Timeout>30</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
