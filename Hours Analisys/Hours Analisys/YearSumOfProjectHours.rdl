    <DataSet Name="MAGMA">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     TOP (100) PERCENT dbo.HoursToEmployee_Ta.ReportID, MONTH(dbo.HoursToEmployee_Ta.ReportDate) AS ReportMonth, 
                      YEAR(dbo.HoursToEmployee_Ta.ReportDate) AS ReportYear, dbo.Employee_Ta.FirstName + N' ' + dbo.Employee_Ta.LastName AS EmployeeName, 
                      dbo.HoursToEmployee_Ta.TaskCode, dbo.ALL_Tasks_Ve.ShortDescription AS Description, dbo.ALL_Tasks_Ve.CompName AS CompanyName, 
                      dbo.HoursToEmployee_Ta.ReportDate, dbo.HoursToEmployee_Ta.HoursToEmployee, dbo.HoursToEmployee_Ta.Comment, 
                      dbo.ALL_Tasks_Ve.PlannedHours, dbo.ActionType_Ta.Description AS ActionType, dbo.ActionType_Ta.ChargingCode, 
                      dbo.HoursToEmployee_Ta.WasDrive
FROM         dbo.HoursToEmployee_Ta INNER JOIN
                      dbo.ALL_Tasks_Ve ON dbo.HoursToEmployee_Ta.TaskCode = dbo.ALL_Tasks_Ve.TaskCode INNER JOIN
                      dbo.Employee_Ta ON dbo.HoursToEmployee_Ta.EmployeeCode = dbo.Employee_Ta.ID LEFT OUTER JOIN
                      dbo.ActionType_Ta ON dbo.HoursToEmployee_Ta.ActionType = dbo.ActionType_Ta.Code
WHERE     (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intReportMonth = 0) AND (@intReportYear = 0) AND 
                      (@intTaskCode = 0) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intReportYear = 0) AND (@intTaskCode = 0) AND 
                      (MONTH(dbo.HoursToEmployee_Ta.ReportDate) = @intReportMonth) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intReportMonth = 0) AND (@intTaskCode = 0) AND 
                      (YEAR(dbo.HoursToEmployee_Ta.ReportDate) = @intReportYear) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intTaskCode = 0) AND 
                      (MONTH(dbo.HoursToEmployee_Ta.ReportDate) = @intReportMonth) AND (YEAR(dbo.HoursToEmployee_Ta.ReportDate) = @intReportYear) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intReportMonth = 0) AND (@intReportYear = 0) AND 
                      (dbo.HoursToEmployee_Ta.TaskCode = @intTaskCode) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intReportYear = 0) AND 
                      (MONTH(dbo.HoursToEmployee_Ta.ReportDate) = @intReportMonth) AND (dbo.HoursToEmployee_Ta.TaskCode = @intTaskCode) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (@intReportMonth = 0) AND 
                      (YEAR(dbo.HoursToEmployee_Ta.ReportDate) = @intReportYear) AND (dbo.HoursToEmployee_Ta.TaskCode = @intTaskCode) OR
                      (dbo.HoursToEmployee_Ta.Absence IS NULL) AND (dbo.ALL_Tasks_Ve.TaskStatus = 13) AND (MONTH(dbo.HoursToEmployee_Ta.ReportDate) 
                      = @intReportMonth) AND (YEAR(dbo.HoursToEmployee_Ta.ReportDate) = @intReportYear) AND (dbo.HoursToEmployee_Ta.TaskCode = @intTaskCode)
ORDER BY dbo.HoursToEmployee_Ta.ReportDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@intReportYear">
            <Value>=Parameters!intReportYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@intReportMonth">
            <Value>=Parameters!intReportMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@intTaskCode">
            <Value>=Parameters!intTaskCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>30</Timeout>
        <DataSourceName>MAGMA</DataSourceName>
      </Query>
